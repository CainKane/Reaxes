{
	"include" : [
		"packages"
	] ,
	"compilerOptions" : {
		
		"allowJs" : true ,
		"skipLibCheck" : true ,
		"allowSyntheticDefaultImports" : true ,
		"allowUnusedLabels" : true ,
		"baseUrl" : "." ,
		"declaration" : true ,
		"esModuleInterop" : true ,
		"experimentalDecorators" : true ,
		"jsx" : "react-jsx" ,
		"resolveJsonModule" : true ,
		"outDir" : "npm/dist" ,
		"strictFunctionTypes" : false ,
		"strict" : false ,
		"strictBindCallApply" : false ,
		"strictNullChecks" : false ,
		"allowUnreachableCode" : true ,
		"module" : "ESNext" ,
		"moduleResolution" : "node" ,
		"emitDeclarationOnly" : true ,
		"noImplicitAny" : false ,
		"noImplicitReturns" : false ,
		"noImplicitUseStrict" : false ,
		"noUnusedLocals" : false ,
		"noUnusedParameters" : false ,
		"paths" : {
			"mobx-react" : [
				"packages/mobxs/mobx-react"
			] ,
			"mobx-react-lite" : [
				"packages/mobxs/mobx-react-lite"
			] ,
			"reaxes" : [
				"packages/core/index"
			] ,
			"@@packages/*" : [
				"packages/*"
			] ,
			"@@RootPath/*" : [
				"*"
			] ,
			"@@utils" : [
				"packages/utils/index"
			] ,
			"@@utils/*" : [
				"packages/utils/*"
			] ,
			"@@Public/*" : [
				"Public/*"
			]
		} ,
		"preserveConstEnums" : true ,
		"removeComments" : true ,
		"target" : "ES2020" ,
		"allowSyntheticDefaultImports" : true ,
		"lib" : [
			"ES5" ,
			"ES6" ,
			"ES2015" ,
			"ES7" ,
			"ES2016" ,
			"ES2017" ,
			"ES2018" ,
			"ESNext" ,
			"DOM.Iterable" ,
			"WebWorker" ,
			"ScriptHost" ,
			"ES2015.Core" ,
			"ES2015.Collection" ,
			"ES2015.Generator" ,
			"ES2015.Iterable" ,
			"ES2015.Promise" ,
			"ES2015.Proxy" ,
			"ES2015.Reflect" ,
			"ES2015.Symbol" ,
			"ES2015.Symbol.WellKnown" ,
			"ES2016.Array.Include" ,
			"ES2017.object" ,
			"ES2017.Intl" ,
			"ES2017.SharedMemory" ,
			"ES2017.String" ,
			"ES2017.TypedArrays" ,
			"ES2018.Intl" ,
			"ES2018.Promise" ,
			"ES2018.RegExp" ,
			"ESNext.AsyncIterable" ,
			"ESNext.Array" ,
			"ESNext.Intl" ,
			"ESNext.Symbol" ,
			"DOM"
		]
	} ,
	"exclude" : [
		"node_modules" ,
		"node_modules/*" ,
		"node_modules/**" ,
		"**/node_modules/**" ,
		"npm" ,
		".git" ,
		".idea" ,
		".svn"
	]
}
